def twonumbersum(array, targetsum):
    seen = set()

    for i in range(len(array)):
        num = array[i]
        difference = targetsum - num
        
        if difference in seen:
            return [difference, num]

        seen.add(num)

    return []

# Sample Input
array = [3, 5, -4, 8, 11, 1, -1, 6]
targetsum = 10
result = twonumbersum(array, targetsum)
print(result)

# For nun in array variant | for element in iterable | best when the need is to access the elements of the iterable exclusively

def twonumbersum(array, targetsum):
    seen = set()

    for num in array:
        difference = targetsum - num
        
        if difference in seen:
            return [difference, num]

        seen.add(num)
        print(seen)
    return []

# Sample Input
array = [3, 5, -4, 8, 11, 1, -1, 6]
targetsum = 10
result = twonumbersum(array, targetsum)
print(result)


array = [3, 5, -4, 9, 11, 1, -1, 6]
targetsum = 2
result = twonumbersum(array, targetsum)
print(result)

def twonumbersum(array, targetsum):
    seen = set()
    
    for num in array:
        difference = targetsum - num
        
        if difference in seen:
            return [difference, num]
        
        seen.add(num)
    return []    

print('Hello World')
array = [3, 5, -4, 9, 11, 1, -1, 6]
targetsum = 2
result = twonumbersum(array, targetsum)
print(result)